import turtle

t= turtle.Turtle()
def main():
  t.color('red')
  t.begin_fill()
  move2location(20,-150)
  medic()
  t.endfill()
  
t.begin_fill()
t.rt(90)
t.fd(50)
t.lt(90)
t.fd(55)
t.rt(90)
t.fd(40)
t.rt(90)
t.fd(110)
t.rt(90)
t.fd(200)
t.rt(90)
t.fd(110)
t.rt(90)
t.fd(40)
t.rt(90)
t.fd(55)
t.lt(90)
t.fd(50)
t.lt(90)
t.fd(40)
t.rt(90)
t.fd(20)
t.rt(90)
t.fd(40)
t.end_fill()
